{
  "name": "svelte-infinite-viewer",
  "version": "0.14.2",
  "description": "Svelte Infinite Viewer is Document Viewer Component with infinite scrolling.",
  "main": "./dist/infinite-viewer.cjs.js",
  "module": "./dist/infinite-viewer.esm.js",
  "types": "src/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c rollup.build.config.js && print-sizes ./dist",
    "dev": "rollup -c -w",
